# Free Tier Configuration - $0/month for 100 users
# Copy to .env and fill in your values

# === INFRASTRUCTURE (FREE) ===
REDIS_URL=redis://default:password@redis-xxxxx.cloud.redislabs.com:12345
DATABASE_URL=postgresql://postgres:password@db.supabase.co:5432/postgres

# === WORKER CONFIG ===
WORKER_ID=worker_1
NUM_USERS=50

# === OPTIMIZATION (Aggressive to stay under free limits) ===
SCAN_INTERVAL=900  # 15 minutes
PRICE_CACHE_TTL=600  # 10 minutes
STRATEGY_CACHE_TTL=300  # 5 minutes
MAX_CONVERSATION_HISTORY=5  # Reduce memory
LOG_LEVEL=WARNING  # Less logging
RPC_BATCH_SIZE=25
SCAN_STAGGER_WINDOW=300

# === SOLANA & APIS ===
SOLANA_NETWORK=mainnet-beta
GROQ_API_KEY=your_groq_key_here

# === HELIUS API KEYS (3 is enough to start!) ===
# Sign up 3 times at helius.dev with different emails
HELIUS_API_KEY_1=key1_here
HELIUS_API_KEY_2=key2_here
HELIUS_API_KEY_3=key3_here

# Optional: Add more keys later when scaling past 75 users
# HELIUS_API_KEY_4=key4_here
# HELIUS_API_KEY_5=key5_here

# === TELEGRAM ===
TELEGRAM_BOT_TOKEN=your_bot_token_here

# === RISK MANAGEMENT (Conservative for free tier) ===
MAX_POSITION_PCT=0.03
HIGH_RISK_POSITION_PCT=0.01
MEDIUM_RISK_POSITION_PCT=0.03
LOW_RISK_POSITION_PCT=0.05
MAX_DAILY_LOSS_PCT=0.05

# === PLATFORM FEE ===
PLATFORM_WALLET=BnepSp5cyDkpszTMfrq3iVEH6cMpiappY2hLxTjjLYyc
MONTHLY_FEE_PERCENTAGE=0.02  # 2% of monthly profit
